ute path=\"/admin/users/:id\" component={UserProfile} />\n                <Route path=\"/admin/newUser\" component={UserProfile} />\n                <Route path=\"/admin/products\" component={ProductList} />\n                <Route path=\"/admin/newProduct\" component={SingleProduct} />\n                <Route path=\"/admin/product_detail/:id\" component={SingleProduct} />\n              </Switch>\n            </div>\n          </div>\n        </Router>\n      ) : (\n        <Router>\n          <Header />\n          <Switch>\n            <Route exact path=\"/\" component={Home} />\n            <Route path=\"/login\" component={ViewLogin} />\n            <Route path=\"/cart\" component={Cart} />\n            <Route path=\"/checkout\" component={Checkout} />\n            <Route path=\"/users/:id\" component={UserProfile} />\n            <Route path=\"/detail/:id\" component={ProductDetail} />\n            <Route path=\"/products\" component={ProductList1} />\n          </Switch>\n          <Footer />\n        </Router>\n      )\n      }\n\n\n    </div >\n  );\n}\n\nexport default App;\n","/home/leduyvuong/Project/drinks_foods_GT01_LarVue/fooddrink/src/translation/i18n.js",[],"/home/leduyvuong/Project/drinks_foods_GT01_LarVue/fooddrink/src/i18n.tsx",[],["221","222"],"/home/leduyvuong/Project/drinks_foods_GT01_LarVue/fooddrink/src/components/Index.tsx",[],["223","224"],"/home/leduyvuong/Project/drinks_foods_GT01_LarVue/fooddrink/src/i18n.js",[],["225","226"],"/home/leduyvuong/Project/drinks_foods_GT01_LarVue/fooddrink/src/components/share/Pagination.tsx",["227","228","229","230","231"],"import React, { useEffect, useState } from 'react';\n\ninterface PaginationProps {\n  total: number,\n  handleClickPage: (page: number) => void,\n  limit: number,\n  page: number\n};\n\nconst Pagination: React.FC<PaginationProps> = props => {\n  const total = props.total;\n\n  let totalProducts = 0;\n  if (total > props.limit)\n    totalProducts = total % props.limit === 0 ? Math.floor(t